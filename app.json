{
  "expo": {
    "scheme": "expo-twitter",
    "icon": "./assets/app-icon.png",
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "name": "Expo Twitter",
    "slug": "twitter",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://expo-twitter.netlify.app"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "app.bacon.twitter",
      "associatedDomains": [
        "applinks:expo-twitter.netlify.app",
        "webcredentials:expo-twitter.netlify.app",
        "activitycontinuation:expo-twitter.netlify.app"
      ]
    }
  }
}
